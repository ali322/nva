    "ejs": "^2.6.1",
    "execa": "^0.6.3",
    "fs-extra": "^2.1.2",
    "glob": "^7.1.2",
    "inquirer": "^3.0.6",
    "js-yaml": "^3.8.3",
    "lodash": "^4.17.4",
    "nva-task": "0.5.75",
    "ora": "^1.2.0",
    "pretty-error": "^2.1.1",
    "rimraf": "^2.5.4",
    "semver": "^5.3.0"
  },
  "devDependencies": {
