{
  "name": "nva-task",
  "version": "0.3.202",
  "description": "tasks for nva",
  "main": "dist/index.js",
  "jsnext:main": "es/index.js",
  "repository": "https://github.com/ali322/nva/tree/master/packages/nva-task",
  "files": [
    "dist",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist es",
    "build": "npm run build:commonjs && npm run build:es",
    "prepublish": "npm run clean && npm run build",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --plugins add-module-exports src --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "lint": "eslint src"
  },
  "keywords": [
    "nva",
    "webpack"
  ],
  "author": "alichen",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "browser-sync": "^2.18.13",
    "chalk": "^1.1.3",
    "chokidar": "^1.7.0",
    "chunk-transform-webpack-plugin": "0.0.17",
    "copy-webpack-plugin": "^4.0.1",
    "del": "^2.2.2",
    "fs-extra": "^2.0.0",
    "glob": "^7.1.2",
    "inject-html-webpack-plugin": "0.0.31",
    "lodash": "^4.17.4",
    "node-emoji": "^1.8.1",
    "nodemon": "^1.11.0",
    "nva-core": "0.3.202",
    "nva-server": "0.3.202",
    "opn": "^5.0.0",
    "portfinder": "^1.0.13",
    "progress-webpack-plugin": "0.0.12",
    "tidy-errors-webpack-plugin": "0.0.1",
    "webpack": "^2.4.1",
