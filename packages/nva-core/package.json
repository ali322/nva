{
  "name": "nva-core",
  "version": "0.2.59",
  "description": "nva core module",
  "repository": "https://github.com/ali322/nva/tree/master/packages/nva-core",
  "main": "dist/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist es",
    "build": "npm run build:commonjs && npm run build:es",
    "prepublish": "npm run clean && npm run build",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --plugins add-module-exports src --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "alichen",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "cross-env": "^3.2.3",
    "eslint-cli": "^1.1.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "autoprefixer": "^6.7.6",
    "babel-core": "^6.24.1",
    "chalk": "^1.1.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "happypack": "^3.0.3",
    "postcss-sprites": "^4.2.0",
    "progress-bar-webpack-plugin": "^1.9.3",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.4.0"
  }
}
